'.source.g':
	editor:
		commentStart: '// '
		increaseIndentPattern: '^.*(\\bcase\\b.*:|\\bdefault\\b:|(\\b(func|if|else|switch|for|struct|interface)\\b.*)?{[^}]*|\\([^)]*)$'
		decreaseIndentPattern: '^\\s*(\\bcase\\b.*:|\\bdefault\\b:|}[),]?|\\)[,]?)$'
		decreaseNextIndentPattern: '^\\s*[^\\s()}]+(?<m>[^()]*\\((?:\\g<m>[^()]*|[^()]*)\\))*[^()]*\\)[,]?$'
